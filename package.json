{
  "name": "package-health-backend",
  "version": "0.0.1",
  "description": "Package Health API",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "node --max-old-space-size=480 dist/src/main.js",
    "start:prod": "node --max-old-space-size=480 dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clean": "rimraf dist tmp"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@sentry/nestjs": "^10.19.0",
    "@sentry/node": "^10.19.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "8.19.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "typescript": "^5.7.3",
    "rimraf": "^6.0.1"
  }
}
